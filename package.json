{
  "name": "micro-class",
  "version": "1.6.2",
  "author": "shewulong",
  "license": "MIT",
  "description": "扩展 js/ts 的 工具类 和 数据结构",
  "keywords": [
    "oop",
    "data structure",
    "algorithm",
    "js",
    "ts",
    "class"
  ],
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "repository": "https://github.com/wulongshe/micro-class.git",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm --minify --clean && rimraf dist/index.d.mts",
    "test": "vitest run"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.5.6",
    "@types/object-hash": "^3.0.3",
    "rimraf": "^5.0.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.23.4"
  },
  "dependencies": {
    "@types/lodash": "^4.14.197",
    "lodash": "^4.17.21",
    "object-hash": "^3.0.0"
  }
}
